		"@tarojs/components": "3.0.5",
		"mobx": "^4.8.0",
		"mobx-react": "^6.1.4",
		"react-dom": "^16.10.0",
		"react": "^16.10.0",
		"anime-sachedule-search-data": "^0.1.1",
		"babel-runtime": "^6.26.0",
		"blueimp-md5": "^2.16.0",
		"lodash": "^4.17.15",
		"nerv-devtools": "^1.5.7",
		"nervjs": "^1.5.7",
		"regenerator-runtime": "0.11.1",
		"taro-ui": "3.0.0-alpha.3"
	},
	"devDependencies": {
		"@tarojs/cli": "3.0.5",
		"@types/webpack-env": "^1.13.6",
